{
    "name" : "visifo-php/nova-enum-field",
    "description" : "A Laravel Nova field to add enums to resources.",
    "version": "4.0.0",
    "authors" : [
        {
            "name" : "Matthew Poulter",
            "email" : "matthew.poulter@simplesquid.co.za",
            "homepage" : "https://simplesquid.co.za/",
            "role" : "Developer"
        },
        {
            "name" : "Sergej Tihonov",
            "email" : "sergej@visifo.com",
            "homepage" : "https://visifo.com/",
            "role" : "Developer"
        }
    ],
    "keywords" : [
        "simplesquid",
        "laravel",
        "nova",
        "field",
        "enum",
        "nova-enum-field"
    ],
    "license" : "MIT",
    "repositories" : [
        {
            "type" : "composer",
            "url" : "https://nova.laravel.com"
        }
    ],
    "require" : {
        "php" : "^8.3",
        "bensampo/laravel-enum" : "^6.0",
        "illuminate/support" : "^11.0",
        "laravel/nova" : "^4.0"
    },
    "require-dev" : {
        "laravel/pint" : "^1.17.2"
    },
    "autoload" : {
        "psr-4" : {
            "SimpleSquid\\Nova\\Fields\\Enum\\" : "src"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : []
        }
    },
    "config" : {
        "sort-packages" : true
    },
    "minimum-stability" : "stable"
}
